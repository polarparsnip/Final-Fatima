<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<?import java.net.URL?>
<AnchorPane prefHeight="500.0" prefWidth="600.0" style="-fx-background-color: #000000;" styleClass="settings" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hi.verkefni.vidmot.SettingsController">
    <children>
        <Button styleClass="menuButtons" fx:id="musicButton" layoutX="199.0" layoutY="167.0" mnemonicParsing="false" onAction="#disableMusicHandler" prefHeight="41.0" prefWidth="204.0" style="-fx-background-color: red; -fx-border-width: 3; -fx-border-color: white; -fx-background-insets: 2; -fx-cursor: hand; -fx-border-radius: 9;" text="Disable music">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Button>
        <Button styleClass="menuButtons" layoutX="199.0" layoutY="310.0" mnemonicParsing="false" onAction="#menuHandler" prefHeight="42.0" prefWidth="204.0" style="-fx-background-color: red; -fx-border-width: 3; -fx-border-color: white; -fx-background-insets: 2; -fx-cursor: hand; -fx-border-radius: 9;" text="Return">
            <font>
                <Font name="System Bold" size="16.0" />
            </font>
        </Button>
        <TextField fx:id="fxSettingsText" alignment="CENTER" editable="false" layoutX="92.0" layoutY="22.0" prefHeight="102.0" prefWidth="417.0" style="-fx-background-color: transparent; -fx-text-fill: red;" text="Settings">
            <font>
                <Font name="System Bold" size="48.0" />
            </font>
        </TextField>
      <Button styleClass="menuButtons" fx:id="sfxButton" layoutX="199.0" layoutY="239.0" mnemonicParsing="false" onAction="#disableSfxHandler" prefHeight="41.0" prefWidth="204.0" style="-fx-background-color: red; -fx-border-width: 3; -fx-border-color: white; -fx-background-insets: 2; -fx-cursor: hand; -fx-border-radius: 9;" text="Disable sfx">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
    </children>
    <stylesheets>
        <URL value="@styles.css" />
    </stylesheets>
</AnchorPane>
